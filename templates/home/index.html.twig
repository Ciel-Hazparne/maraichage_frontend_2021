{% extends 'base.html.twig' %}

{% block title %}Accueil{% endblock %}

{% block body %}
<div class="container">

    <h1>Produits de la ferme</h1>
    <div class="row m-5">
        {% for produit in produits %}

            <div class="col-md-3">
                <div class="card text-white bg-primary mb-3" style="max-width: 20rem;">
                    <img class="card-img-top" src="data:image/jpeg;base64," alt="">

                    <div class="card-body">
                        <h4 class="card-title">{{ produit.nom }}</h4>
                        <p class="card-text"> Prix: {{ produit.prix }} €</p>
                        <p class="card-text">Quantité: {{ produit.quantite}} </p>
                        {% if app.user %}
                        <a href="{{ path('panier_add', {'id' : produit.id})}}" >
                            <button type="button" class="btn btn-outline-success">Ajouter au panier</button> </a>
                        {% endif %}
                    </div>
                </div>
            </div>

        {% endfor %}

    </div>

    {{ knp_pagination_render(produits) }}
    {% endblock %}
